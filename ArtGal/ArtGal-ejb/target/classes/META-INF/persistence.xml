<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="ArtGal-ejb">
		<jta-data-source>java:jboss/datasources/ArtGarDB</jta-data-source>
		<class>entities.Artwork</class>
		<class>entities.AspNetRole</class>
		<class>entities.AspNetUser</class>
		<class>entities.AspNetUserClaim</class>
		<class>entities.AspNetUserLogin</class>
		<class>entities.AspNetUserLoginPK</class>
		<class>entities.Bid</class>
		<class>entities.BidPK</class>
		<class>entities.Category</class>
		<class>entities.Comment</class>
		<class>entities.CommentLike</class>
		<class>entities.CommentLikePK</class>
		<class>entities.Deal</class>
		<class>entities.DealAchete</class>
		<class>entities.DealAchetePK</class>
		<class>entities.GalleryAssociation</class>
		<class>entities.SubCategory</class>
		<class>entities.Topic</class>
		<class>entities.TopicCategory</class>
		<class>entities.TopicLike</class>
		<class>entities.TopicLikePK</class>
		<class>entities.UserConnection</class>
		<class>entities.VenteAuxEnchere</class>
		<properties>
			<property name="hibernate.hbm2ddl.auto" value="update" />
		</properties>
	</persistence-unit>
</persistence>
